{
  "kind": "collectionType",
  "collectionName": "ordens",
  "info": {
    "singularName": "orden",
    "pluralName": "ordens",
    "displayName": "Orden"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "NumeroOrden": {
      "type": "integer",
      "required": true
    },
    "Fecha": {
      "type": "date",
      "required": true
    },
    "Total": {
      "type": "float",
      "required": true
    },
    "Estado": {
      "type": "enumeration",
      "enum": [
        "Pendiente",
        "En Proceso",
        "Entregado",
        "Cancelado"
      ]
    },
    "EntregaEsperada": {
      "type": "date"
    },
    "proveedor": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::proveedor.proveedor"
    },
    "empresa": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::empresa.empresa"
    }
  }
}
