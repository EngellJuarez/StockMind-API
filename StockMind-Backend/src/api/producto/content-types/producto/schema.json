{
  "kind": "collectionType",
  "collectionName": "productos",
  "info": {
    "singularName": "producto",
    "pluralName": "productos",
    "displayName": "Producto"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Nombre": {
      "type": "string"
    },
    "Descripcion": {
      "type": "text"
    },
    "PrecioCompra": {
      "type": "biginteger"
    },
    "PrecioVenta": {
      "type": "biginteger"
    },
    "categoria": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::categoria.categoria"
    },
    "movimientos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::movimiento.movimiento",
      "mappedBy": "producto"
    },
    "empresa": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::empresa.empresa"
    }
  }
}
